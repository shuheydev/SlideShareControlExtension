{
    "manifest_version": 2,
    "name": "SlideShareControl",
    "description": "SignalR経由でSlideShareを操作する拡張機能",
    "version": "1.7",
    "content_scripts": [
        {
          "run_at":"document_end",
          "matches": ["https://www.slideshare.net/*/*"],
          "js": [
            "lib/signalr/dist/browser/signalr.js",
            "lib/jquery/jquery_3.4.1.min.js",
            "content_script.js"]
        }
      ],
    "permissions": [
      "activeTab",
      "declarativeContent"
    ]
}